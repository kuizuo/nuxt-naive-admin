generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  // provider = "sqlite"
  // url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(cuid())
  name     String
  email    String @unique
  password String
  image    String?
}

model GithubUser {
  id         Int      @id @unique
  name       String
  avatar_url String
  followers  Int
  following  Int
  created_at DateTime
  updated_at DateTime
}
